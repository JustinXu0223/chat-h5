    "lint": "eslint --ext .js,.vue src",
    "dll:prepare": "npm run dll:prepare:dev && npm run dll:prepare:prd",
    "dll:prepare:dev": "cross-env NODE_ENV=development gulp dll:prepare",
    "dll:prepare:prd": "cross-env NODE_ENV=production gulp dll:prepare",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js --open",
    "start:dev": "cross-env SERVER_IP=http://www.xxx.com npm run dev",
    "start:test": "cross-env SERVER_IP=http://m.xxx.com npm run dev",
    "analysis": "cross-env NODE_ENV=development MODE=analyse npm run dev",
    "release": "cross-env NODE_ENV=production ENVIRONMENT=staging gulp release",
    "release:production": "cross-env NODE_ENV=production ENVIRONMENT=production gulp release"
  },
  "pre-commit": [
    "precommit-msg",
    "lint:style",
    "lint"
